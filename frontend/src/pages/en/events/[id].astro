---
import { RuiDashboardApp } from "@/components/dashboard/RuiDashboardApp";
import Layout from "@/layouts/Layout.astro";

export const prerender = false;

const { id } = Astro.params;
const initialPath = `/en/events/${id}`;
---

<Layout title="Event Management - CIP Events" lang="en">
  <RuiDashboardApp client:only="react" initialPath={initialPath} />
</Layout>
